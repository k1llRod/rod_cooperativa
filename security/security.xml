<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.module.category" id="module_rod_cooperativa">
        <field name="name">Cooperativa Prestamos</field>
        <field name="description">Rol de usuarios para Cooperativa Prestamos</field>
        <field name="sequence">46</field>
    </record>
    <record id="group_rod_cooperativa_user" model="res.groups">
        <field name="name">Usuario</field>
        <field name="category_id" ref="module_rod_cooperativa"/>
    </record>
    <record id="group_rod_cooperativa_operator" model="res.groups">
        <field name="name">Operador</field>
        <field name="category_id" ref="module_rod_cooperativa"/>
        <field name="implied_ids" eval="[(4, ref('rod_cooperativa.group_rod_cooperativa_user'))]"/>
    </record>
    <record id="group_rod_cooperativa_administrator" model="res.groups">
        <field name="name">Administrador</field>
        <field name="category_id" ref="module_rod_cooperativa"/>
        <field name="implied_ids"
               eval="[(4, ref('rod_cooperativa.group_rod_cooperativa_user')),(4, ref('rod_cooperativa.group_rod_cooperativa_operator'))]"/>
    </record>
</odoo>