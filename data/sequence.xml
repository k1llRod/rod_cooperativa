<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_loan_application" model="ir.sequence">
            <field name="name">Secuencia de solicitud de prestamos</field>
            <field name="code">loan.application</field>
            <field name="prefix">COA4</field>
            <field name="padding">4</field>
        </record>
        <record id="seq_loan_payment" model="ir.sequence">
            <field name="name">Secuencia de pago de prestamos</field>
            <field name="code">loan.payment</field>
            <field name="prefix">PRESCOA</field>
            <field name="padding">4</field>
        </record>
        <record id="seq_post_mortem" model="ir.sequence">
            <field name="name">Secuencia de pago post mortem</field>
            <field name="code">post.mortem</field>
            <field name="prefix">PSTMORT</field>
            <field name="padding">4</field>
        </record>
    </data>
</odoo>